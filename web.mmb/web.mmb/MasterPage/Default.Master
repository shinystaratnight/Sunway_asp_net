<%@ Master Language="VB" AutoEventWireup="false" CodeBehind="Default.master.vb" Inherits="Web.MMB.Default" %>
<%@ register assembly="IntuitiveWeb" namespace="Intuitive.Web" tagprefix="web" %>

<!DOCTYPE html>
<html>

<web:head id="Head" runat="server"/>

<body id="frm"  runat="server">

    <web:trackingcontrol id="TopBody" runat="server"/>

	<!-- Top Header -->
	<web:container id="divTopHeader" cssclass="clear" runat="server"></web:container>

	<web:container id="divSubTopHeader" cssclass="clear" runat="server"></web:container>

	<web:container id="divAll" cssclass="clear container" runat="server">
		
		<web:container id="divMain" cssclass="clear" runat="server">

			<!-- Header -->
			<web:container id="divHeader" cssclass="clear" runat="server"></web:container>

			<!-- Subheader -->
			<web:container id="divSubHeader" cssclass="clear" runat="server"></web:container>

			<!-- Sidebar -->
			<web:container id="divSidebar" cssclass="clear four columns alpha" runat="server"></web:container>
            
             <!-- Content -->
		    <web:container id="divContent" cssclass="clear twelve columns omega" runat="server"></web:container>

            <!-- SidebarRight -->
			<web:container id="divSidebarRight" cssclass="clear six columns omega" runat="server"></web:container>

            <!-- ContentLeft -->
		    <web:container id="divContentLeft" cssclass="clear ten columns alpha" runat="server"></web:container>

             <!-- Full Width Content -->
		    <web:container id="divContentFull" cssclass="clear" runat="server"></web:container>		
			
		</web:container>
			
	</web:container>

    <!-- Extended content for full screen width -->
    <web:container id="divContentExtended" cssclass="clear" runat="server"></web:container>

	<!-- Footer -->
	<web:container id="divFooter" cssclass="clear" runat="server"></web:container>

    <web:trackingcontrol id="BottomBody" runat="server"/>
	
</body>

</html>
